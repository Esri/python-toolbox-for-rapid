<metadata xml:lang="en"><Esri><CreaDate>20160708</CreaDate><CreaTime>10131700</CreaTime><ArcGISFormat>1.0</ArcGISFormat><SyncOnce>TRUE</SyncOnce><ModDate>20160721</ModDate><ModTime>10052000</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>ItemDescription</ArcGISProfile></Esri><tool name="CreateNetworkConnectivityFileNHDPlus" displayname="Create Connectivity File for NHDPlus" toolboxalias="RAPIDTools" xmlns=""><arcToolboxHelpPath>c:\program files (x86)\arcgis\desktop10.3\Help\gp</arcToolboxHelpPath><parameters><param name="in_drainage_line_features" displayname="Input Drainage Line Features" type="Required" direction="Input" datatype="Feature Layer" expression="in_drainage_line_features"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;These are the Drainage Line features that contain the COMID, FROMNODE, TONODE, and DIVERGENCE fields.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="stream_ID" displayname="Stream ID" type="Required" direction="Input" datatype="Field" expression="stream_ID"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;This is the unique ID associates with the stream reach. For the NHDPlus it is usually COMID.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="fromnode_field" displayname="From Node ID" type="Required" direction="Input" datatype="Field" expression="fromnode_field"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;This is the FROMNODE field in the NHDPlus attributes. This may need to be added from the extended attribute tables.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="tonode_field" displayname="To Node ID" type="Required" direction="Input" datatype="Field" expression="tonode_field"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN